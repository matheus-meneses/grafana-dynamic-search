{
  "name": "grafana-dynamic-search",
  "version": "1.1.0",
  "scripts": {
    "build": "webpack -c ./.config/webpack/webpack.config.ts --env production",
    "dev": "webpack -w -c ./.config/webpack/webpack.config.ts --env development",
    "test": "jest --watch --onlyChanged",
    "test:ci": "jest --passWithNoTests --maxWorkers 4",
    "test:coverage": "jest --passWithNoTests --maxWorkers 4 --coverage",
    "test:perf": "jest --testMatch='**/*.bench.ts' --verbose",
    "test:all": "npm run test:ci && npm run e2e",
    "typecheck": "tsc --noEmit",
    "lint": "eslint --cache .",
    "lint:fix": "npm run lint -- --fix && prettier --write --list-different .",
    "e2e": "playwright test",
    "server": "docker compose up --build",
    "sign": "npx --yes @grafana/sign-plugin@latest --rootUrls http://localhost:3000"
  },
  "author": "Matheus Meneses",
  "license": "Apache-2.0",
  "devDependencies": {
    "@grafana/eslint-config": "^9.0.0",
    "@grafana/plugin-e2e": "^3.1.0",
    "@grafana/tsconfig": "^2.0.1",
    "@jest/test-sequencer": "^30.2.0",
    "@playwright/test": "^1.52.0",
    "@stylistic/eslint-plugin": "^4.4.1",
    "@swc/core": "^1.14.0",
    "@swc/helpers": "^0.5.0",
    "@swc/jest": "^0.2.26",
    "@testing-library/jest-dom": "6.9.1",
    "@testing-library/react": "^16.3.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.8.7",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^8.3.0",
    "@typescript-eslint/parser": "^8.3.0",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^7.1.2",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-jsdoc": "^61.5.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.0",
    "eslint-webpack-plugin": "^5.0.0",
    "fork-ts-checker-webpack-plugin": "^9.1.0",
    "glob": "^10.2.7",
    "identity-obj-proxy": "3.0.0",
    "imports-loader": "^5.0.0",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "prettier": "^2.8.7",
    "replace-in-file-webpack-plugin": "^1.0.6",
    "sass": "1.63.2",
    "sass-loader": "13.3.1",
    "semver": "^7.6.3",
    "style-loader": "4.0.0",
    "swc-loader": "^0.2.3",
    "terser-webpack-plugin": "^5.3.10",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "webpack": "^5.104.1",
    "webpack-cli": "^5.1.4",
    "webpack-livereload-plugin": "^3.0.2",
    "webpack-subresource-integrity": "^5.1.0",
    "webpack-virtual-modules": "^0.6.2"
  },
  "engines": {
    "node": ">=22"
  },
  "dependencies": {
    "@emotion/css": "11.10.6",
    "@grafana/data": "^12.3.1",
    "@grafana/i18n": "^12.3.0",
    "@grafana/runtime": "^12.3.1",
    "@grafana/schema": "^12.3.1",
    "@grafana/ui": "^12.3.1",
    "react": "^18.3.0",
    "react-dom": "^18.3.0"
  },
  "packageManager": "npm@11.6.2"
}
